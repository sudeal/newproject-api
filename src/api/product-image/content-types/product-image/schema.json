{
  "kind": "collectionType",
  "collectionName": "product_images",
  "info": {
    "singularName": "product-image",
    "pluralName": "product-images",
    "displayName": "ProductImage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "alt": {
      "type": "string"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "product_image"
    }
  }
}
