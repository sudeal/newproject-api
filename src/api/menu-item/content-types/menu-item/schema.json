{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "MenuItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "restaurant": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::restaurant.restaurant",
      "inversedBy": "menu_item"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "mappedBy": "menu_item"
    }
  }
}
